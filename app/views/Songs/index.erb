<% @songs.each do |song| %>
  <div class="song">
    <h3><a href="/songs/<%= song.id %>"><%= song.title %></a> <small>(by: <%= song.author%>)</small></h3>
    <p>
      <%= song.url %>
      <small>created by: <%= song.user.name %> at: <%= song.created_at.strftime('%B %e, %Y' ) %></small>
    </p>
  </div>

<% end %>
